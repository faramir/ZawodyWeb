<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
"http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
    <application>
        <message-bundle>pl.umk.mat.zawodyweb.www.Messages</message-bundle>
        <locale-config>
            <default-locale>pl_PL</default-locale>
        </locale-config>
    </application>
    <managed-bean>
        <managed-bean-name>sessionBean</managed-bean-name>
        <managed-bean-class>pl.umk.mat.zawodyweb.www.SessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>rolesBean</managed-bean-name>
        <managed-bean-class>pl.umk.mat.zawodyweb.www.RolesBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>sessionBean</property-name>
            <value>#{sessionBean}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>requestBean</managed-bean-name>
        <managed-bean-class>pl.umk.mat.zawodyweb.www.RequestBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>sessionBean</property-name>
            <value>#{sessionBean}</value>
        </managed-property>
        <managed-property>
            <property-name>rolesBean</property-name>
            <value>#{rolesBean}</value>
        </managed-property>
        <managed-property>
            <property-name>temporaryResultId</property-name>
            <value>#{param.temporaryResultId}</value>
        </managed-property>
        <managed-property>
            <property-name>ratingEditNote</property-name>
            <value>#{param.ratingEditNote}</value>
        </managed-property>
    </managed-bean>
    <navigation-rule>
        <display-name>register_to_login</display-name>
        <from-view-id>/register.jspx</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>login_to_index</display-name>
        <from-view-id>/login.jspx</from-view-id>
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/start.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>to_index</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/start.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>editcontest_to_index</display-name>
        <from-view-id>/admin/editcontest.jspx</from-view-id>
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/start.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>editseries_to_index</display-name>
        <from-view-id>/admin/editseries.jspx</from-view-id>
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/start.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToProblems</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToPasswd</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>goToPasswd</from-outcome>
            <to-view-id>/passwd.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToRanking</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>ranking</from-outcome>
            <to-view-id>/ranking.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToRules</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>rules</from-outcome>
            <to-view-id>/rules.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>questions_to_questions</display-name>
        <from-view-id>/questions.jspx</from-view-id>
        <navigation-case>
            <from-outcome>questions</from-outcome>
            <to-view-id>/questions.jspx</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>question_to_questions</display-name>
        <from-view-id>/question.jspx</from-view-id>
        <navigation-case>
            <from-outcome>questions</from-outcome>
            <to-view-id>/questions.jspx</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToQuestions</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>questions</from-outcome>
            <to-view-id>/questions.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToSubmissions</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>submissions</from-outcome>
            <to-view-id>/submissions.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>submitProblem</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>submit</from-outcome>
            <to-view-id>/submit.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>edit_problem_to_problems</display-name>
        <from-view-id>/admin/editproblem.jspx</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>edit_test_to_problems</display-name>
        <from-view-id>/admin/edittest.jspx</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>submit_to_problems</display-name>
        <from-view-id>/submit.jspx</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>problems_to_problems</display-name>
        <from-view-id>/problems.jspx</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>delproblem_to_problems</display-name>
        <from-view-id>/del/*</from-view-id>
        <navigation-case>
            <from-outcome>problems</from-outcome>
            <to-view-id>/problems.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ToProblem</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>problem</from-outcome>
            <to-view-id>/problem.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ToQuestion</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>question</from-outcome>
            <to-view-id>/question.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ToSubmission</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>submission</from-outcome>
            <to-view-id>/submission.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>passwd_to_profil</display-name>
        <from-view-id>/passwd.jspx</from-view-id>
        <navigation-case>
            <from-outcome>profil</from-outcome>
            <to-view-id>/profil.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>edit_user_to_listusers</display-name>
        <from-view-id>/admin/edituser.jspx</from-view-id>
        <navigation-case>
            <from-outcome>listusers</from-outcome>
            <to-view-id>/admin/listusers.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>edit_class_to_listclasses</display-name>
        <from-view-id>/admin/editclass.jspx</from-view-id>
        <navigation-case>
            <from-outcome>listclasses</from-outcome>
            <to-view-id>/admin/listclasses.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>edit_language_to_listlanguages</display-name>
        <from-view-id>/admin/editlanguage.jspx</from-view-id>
        <navigation-case>
            <from-outcome>listlanguages</from-outcome>
            <to-view-id>/admin/listlanguages.jspx</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goToViewCode</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>view_code</from-outcome>
            <to-view-id>/viewcode.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
