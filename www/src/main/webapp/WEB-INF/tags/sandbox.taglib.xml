<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
    <namespace>http://myfaces.apache.org/sandbox</namespace>
    <tag>
        <tag-name>captcha</tag-name>
        <component>
            <component-type>org.apache.myfaces.CAPTCHA</component-type>
            <renderer-type>org.apache.myfaces.CAPTCHA</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>validateCompareTo</tag-name>
        <validator>
            <validator-id>org.apache.myfaces.validator.CompareTo</validator-id>
        </validator>
    </tag>
</facelet-taglib>