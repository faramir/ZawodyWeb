<h:panelGroup xmlns:h="http://java.sun.com/jsf/html">
    <h:outputLink value="#{facesContext.externalContext.requestContextPath}/#{type}.html" rendered="#{contest == null}">${messages.problems}</h:outputLink>
    <h:outputLink value="#{facesContext.externalContext.requestContextPath}/#{type}/#{contest.id}/#{zw:filterUri(contest.name)}.html" rendered="#{!(contest == null)}">
        <h:outputText value="#{messages.problems}" rendered="${default == 'true'}" />
        <h:outputText value="#{contest.name}" rendered="${!(default == 'true')}" />
    </h:outputLink>
</h:panelGroup>

